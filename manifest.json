{
  "manifest_version": 2,

  "name": "swede-it",
  "description": "Take a trip to Sweden without leaving your browser!",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "browser_action": {
    "default_popup": "popup.html"
  },

  "permissions": [
    "tabs",
    "file:///Users/*",
    "https://*/",
    "http://*/"
  ]
}
